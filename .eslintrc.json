{
    "extends": "airbnb-base",
    "rules": {
        "semi": ["error", "never"],
        "no-console": "error",
        "no-underscore-dangle" : "off",
        "one-var-declaration-per-line": "off",
        "one-var": "off",
        "no-param-reassign": "off",
        "import/no-unresolved": "off"
    },
    "globals": {
        "log": true,
        "__basedir": true
    }
}