{
    "extends": "airbnb-base",
    "rules": {
        "semi": ["error", "never"],
        "no-console": "error",
        "no-underscore-dangle" : "off",
        "one-var-declaration-per-line": "off",
        "one-var": "off",
        "no-param-reassign": "off",
        "import/no-unresolved": "off",
        "camelcase": "off",
        "arrow-parens": "off",
        "no-plusplus": "off"
    },
    "globals": {
        "log": true,
        "try_": true,
        "Config": true
    }
}