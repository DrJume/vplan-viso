{
  "name": "vplan-viso",
  "version": "0.5.3",
  "description": "Vertretungsplan-Visualisierungssoftware f√ºr den Einsatz in Schulen",
  "main": "index.js",
  "engines": {
    "node": "8.11.x",
    "npm": "6.0.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "test": "echo 'Error: no test specified' && exit 1",
    "reset": "rm -r upload/ ; rm -r logs/ ; rm config.json ; rm Update.tar.gz",
    "build": "npx riot src/riot-tags/ routes/riot/tags.js && npx babel routes/riot/tags.js --out-file routes/riot/tags-compiled.js --source-maps",
    "prestart-dev": "npm run build",
    "start-dev": "node index.js --color",
    "dev": "npx nodemon index.js",
    "start": "npx pm2 start pm2.config.js",
    "stop": "npx pm2 stop pm2.config.js"
  },
